public class Stat{
	
	private double [] data;
	
	public Stat(){
		data = new double[0];
	}
	
	public Stat(double[] d){
		data = d;
	}
	
	public String toString(){
		String s = "";
		if (data.length == 0)
			return "[]";
		
		else{
			s = "[";
			for (int i = 0; i < data.length; i++)
				s += data[i] + ", ";
			
			s += "]";
		}
			
		return s;
	}
}